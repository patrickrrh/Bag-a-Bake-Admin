{
  "expo": {
    "name": "Bag a Bake Admin",
    "slug": "bag-a-bake-admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foregroundBagabake.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.admin.bagabake",
      "minSdkVersion": 24,
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "baseURL": "http://bagabake.com/api",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2725cb0d-48b2-4e96-b1da-c08260b71fc1"
      }
    },
    "owner": "bag-a-bake"
  }
}
